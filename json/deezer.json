{
    "name" : "Deezer",

"COMMENT_1" : "This block contiains all of the things a player may or may not have",

    "has_thumbs_up" : true,
    "has_thumbs_down" : false,
    "has_next_track" : true,
    "has_prev_track" : true,
    "has_play_pause" : true,
    "has_shuffle" : true,
    "has_repeat" : true,
    "has_current_track_time" : true,
    "has_total_track_time" : true,
    "has_remaining_track_time" : false,

"COMMENT_2" : "This block contains all boolean values we can get back from the player",

    "is_playing" : "$('#player_control_play').css('display') == 'none'",
    "is_paused" : "$('#player_control_pause').css('display') == 'none'",
    "is_shuffled" : "$('#player_option_shuffle').hasClass('selected')",
    "is_repeat_off" : "!$('#player_option_repeat').hasClass('selected')",
    "is_repeat_one" : "var a=$('#player_option_repeat'); a.hasClass('selected') && a.hasClass('option-repeat-one')",
    "is_repeat_all" : "var a=$('#player_option_repeat'); a.hasClass('selected') && !a.hasClass('option-repeat-one')",
    "is_thumbed_up" : "$('#player_action_love').hasClass('selected')",

"COMMENT_3" : "This block contains all string values we can get back from the player",
    
    "get_album_art" : "$('#naboo_menu_infos_cover').attr('src')",
    "get_artist" : "$('#player_track_artist').text()",
    "get_track" : "$('#player_track_title').text()",
    "get_current_time" : "$('#player_track_position').text()",
    "get_total_time" : "$('#player_track_length').text()",

"COMMENT_4" : "Finally, this block contains info on how to perform actions",

    "click_play" : "$('#player_control_play').click()",
    "click_pause" : "$('#player_control_pause').click()",
    "click_next_track" : "$('#player_control_next').click()",
    "click_prev_track" : "$('#player_control_prev').click()",
    "click_shuffle" : "$('#player_option_shuffle').click()",
    "click_repeat" : "$('#player_option_repeat').click()",
    "click_thumbs_up" : "$('#player_action_love').click()"
}
