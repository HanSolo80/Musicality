{
    "name" : "Deezer",

"COMMENT_1" : "This block contiains all of the things a player may or may not have",

    "has_thumbs_up" : true,
    "has_thumbs_down" : false,
    "has_next_track" : true,
    "has_prev_track" : true,
    "has_play_pause" : true,
    "has_shuffle" : true,
    "has_repeat" : true,
    "has_current_track_time" : true,
    "has_total_track_time" : true,
    "has_remaining_track_time" : false,

"COMMENT_2" : "This block contains all boolean values we can get back from the player",

    "isPlaying" : "$('#player_control_play').css('display') == 'none'",
    "isPaused" : "$('#player_control_pause').css('display') == 'none'",
    "isShuffled" : "$('#player_option_shuffle').hasClass('selected')",
    "isRepeatOff" : "!$('#player_option_repeat').hasClass('selected')",
    "isRepeatOne" : "var a=$('#player_option_repeat'); a.hasClass('selected') && a.hasClass('option-repeat-one')",
    "isRepeatAll" : "var a=$('#player_option_repeat'); a.hasClass('selected') && !a.hasClass('option-repeat-one')",
    "isThumbedUp" : "$('#player_action_love').hasClass('selected')",

"COMMENT_3" : "This block contains all string values we can get back from the player",
    
    "artUrl" : "$('#naboo_menu_infos_cover').attr('src')",
    "artist" : "$('#player_track_artist').text()",
    "track" : "$('#player_track_title').text()",
    "currentTime" : "$('#player_track_position').text()",
    "totalTime" : "$('#player_track_length').text()",

"COMMENT_4" : "Finally, this block contains info on how to perform actions",

    "click_play" : "$('#player_control_play').click()",
    "click_pause" : "$('#player_control_pause').click()",
    "click_next_track" : "$('#player_control_next').click()",
    "click_prev_track" : "$('#player_control_prev').click()",
    "click_shuffle" : "$('#player_option_shuffle').click()",
    "click_repeat" : "$('#player_option_repeat').click()",
    "click_thumbs_up" : "$('#player_action_love').click()"
}
