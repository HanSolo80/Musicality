{

    "name" : "My Cloud Player",

"COMMENT_1" : "This block contiains all of the things a player may or may not have",

    "has_thumbs_up" : true,
    "has_thumbs_down" : false,
    "has_next_track" : true,
    "has_prev_track" : true,
    "has_play_pause" : true,
    "has_shuffle" : true,
    "has_repeat" : true,
    "has_current_track_time" : true,
    "has_total_track_time" : true,
    "has_remaining_track_time" : false,

"COMMENT_2" : "This block contains all boolean values we can get back from the player",

    "isPlaying" : "$('.playtoggle').hasClass('pause')",
    "isPaused" : "($('.playtoggle').hasClass('play') && $('#staticHeader h3').length > 0)",
    "isShuffled" : "$('.shuffle').hasClass('active');",
    "isRepeatOff" : "$('.repeat.active').length == 0",
    "isRepeatOne" : "$('.repeat.active').length > 0",
    "isRepeatAll" : "$('.repeat.active[orig-title*=All]').length > 0",
    "isThumbedUp" : "$('#trackInfos .favs.favorited').length > 0",
    "isThumbedDown" : "$('#trackInfos .favs.favorited').length == 0",

"COMMENT_3" : "This block contains all string values we can get back from the player",
    
    "artUrl" : "$('#artwork img.active').attr('src')",
    "artist" : "$('#staticHeader h4').text()",
    "track" : "$('#staticHeader h3').text()",
    "currentTime" : "$('#progress').text().split('/')[0].trim()",
    "totalTime" : "$('#progress').text().split('/')[1].trim()",

"COMMENT_4" : "Finally, this block contains info on how to perform actions",

    "click_play" : "$('.playtoggle').click()",
    "click_pause" : "$('.playtoggle').click()",
    "click_next_track" : "$('.html5player_padding .next').click()",
    "click_prev_track" : "$('html5player_padding .prev').click()",
    "click_shuffle" : "$('.html5player_padding .shuffle').click()",
    "click_repeat" : "$('.html5player_padding .repeat').click()",
    "click_thumbs_up" : "$('.html5player_padding .gfavorite').click()",
    "click_thumbs_down" : "$('.html5player_padding .gfavorite').click()"
}
