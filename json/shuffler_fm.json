{

    "name": "Shuffler.fm",

    "COMMENT_1": "This block contains all of the things a player may or may not have",

    "has_thumbs_up": false,
    "has_thumbs_down": false,
    "has_next_track": true,
    "has_prev_track": true,
    "has_play_pause": true,
    "has_shuffle": false,
    "has_repeat": false,
    "has_current_track_time": true,
    "has_total_track_time": false,
    "has_remaining_track_time": true,

    "COMMENT_2": "This block contains all boolean values we can get back from the player",

    "isPlaying": "$('#play-pause').hasClass('pause')",
    "isPaused": "$('#play-pause').hasClass('play')",

    "COMMENT_3": "This block contains all string values we can get back from the player",

    "artUrl" : "",
    "artist": "$('.artist-name').text()",
    "track": "$('.track-title').text()",
    "currentTime": "$('.elapsed').text()",
    "remainingTime": "$('.remaining').text()",

    "COMMENT_4": "Finally, this block contains info on how to perform actions",

    "click_play": "$('#play-pause')[0].click()",
    "click_pause": "$('#play-pause')[0].click()",
    "click_next_track": "$('#next')[0].click()",
    "click_prev_track": "$('#prev')[0].click()"
}