{
    "name" : "Bandcamp",

"COMMENT_1" : "This block contiains all of the things a player may or may not have",

    "has_thumbs_up" : false,
    "has_thumbs_down" : false,
    "has_next_track" : true,
    "has_prev_track" : true,
    "has_play_pause" : true,
    "has_shuffle" : false,
    "has_repeat" : false,
    "has_current_track_time" : true,
    "has_total_track_time" : true,
    "has_remaining_track_time" : false,

"COMMENT_2" : "This block contains all boolean values we can get back from the player",

    "isPlaying" : "$('div.playbutton').hasClass('playing')",
    "isPaused" : "!$('div.playbutton').hasClass('playing')",

"COMMENT_3" : "This block contains all string values we can get back from the player",
    
    "artUrl" : "$('div#tralbumArt').find('img').attr('src')",
    "artist" : "$('span[itemprop=byArtist]').text().trim()",
    "track" : "$('div.desktop-view').find('span.title').text().trim()",
    "currentTime" : "$('div.track_info').find('span.time_elapsed').text()",
    "totalTime" : "$('div.track_info').find('span.time_total').text()",

"COMMENT_4" : "Finally, this block contains info on how to perform actions",

    "click_play" : "$('div.playbutton').click()",
    "click_pause" : "$('div.playbutton').click()",
    "click_next_track" : "$('div.nextbutton').click()",
    "click_prev_track" : "$('div.prevbutton').click()"
}
